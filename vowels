#Write function to return the count of vowels in a string passed as its only parameter.
def checkvowels():
    # a = 
    a = input("Enter the Alphabets: ")
    vowelcount = {}
    for vowel in "aeiou":
        v = a.count(vowel)
        vowelcount[vowel] = v
    print(vowelcount)

